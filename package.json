{
  "name": "rotombot",
  "version": "1.0.0",
  "description": "Discord Bot used for organizing Pokemon Go Raids",
  "main": "rotombot.js",
  "scripts": {
    "test": "jasmine --config=test/support/jasmine.json",
    "cover": "nyc --reporter=lcov --reporter=text jasmine --config=test/support/jasmine.json",
    "lint": "eslint rotombot.js lib commands test/unit",
    "fixlint": "eslint rotombot.js lib commands test/unit --fix",
    "bot": "pm2 start rotombot.js",
    "notbot": "pm2 stop rotombot.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cguerrerCS/rotombot.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cguerrerCS/rotombot/issues"
  },
  "homepage": "https://github.com/cguerrerCS/rotombot#readme",
  "dependencies": {
    "botsbits": "git+https://github.com/DidjaRedo/BotsBits.git",
    "csv-reader": "^1.0.3",
    "discord-command-parser": "^1.2.1",
    "discord.js": "^11.5.0",
    "discord.js-commando": "^0.10.0",
    "fuse.js": "^3.4.4",
    "npm": "^6.9.0",
    "request": "^2.88.0",
    "string-format": "^2.0.0"
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-standard": "^3.1.0",
    "jasmine": "^3.4.0",
    "nyc": "^11.9.0",
    "pm2": "^2.10.4"
  }
